def swap_case(input_str):
    result = ''
    for char in input_str:
        if char.islower():
            result += char.upper()
        elif char.isupper():
            result += char.lower()
        else:
            result += char
    return result

input_str = 'Pythonist+2$'
output_str = swap_case(input_str)
print(output_str)
